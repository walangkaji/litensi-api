{
    "name": "walangkaji/litensi-api",
    "description": "Unofficial API for litensi.id",
    "type": "library",
    "authors": [
        {
            "name": "Aji",
            "homepage": "https://github.com/walangkaji"
        }
    ],
    "require": {
        "php": ">=8.0",
        "walangkaji/mancurl": "^1.1",
        "spatie/data-transfer-object": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^5.26",
        "phpstan/phpstan": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.64",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "walangkaji\\LitensiAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "walangkaji\\LitensiAPITests\\": "tests/"
        }
    }
}
